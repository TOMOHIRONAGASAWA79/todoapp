<%# ヘッダー %>
<header>
  <h1>ToDoLists</h1>
</header>
<%# タスク一覧表示 %>
<h2>Current Taskss</h2>
<table>
  <thead>
    <tr>
      <th>TASK</th>
      <th>DAY</th>
   </tr>
  </thead>
  <tbody>
    <% @tasks.each do |task| %>
      <tr>
        <td><%= task.text %></td>
        <td><%= task.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

  